{
	"name": "Puppeteer Extension",
	"manifest_version": 3,
	"version": "0.1.0",
	"description": "Example extension puppeteer-extension-transport",
	"permissions": ["tabs", "debugger", "activeTab"],
	"host_permissions": ["<all_urls>"],
	"options_page": "options.html",
	"icons": {
		"16": "icon.png",
		"48": "icon.png",
		"128": "icon.png"
	},
	"action": {
		"default_popup": "popup.html",
		"default_icon": {
			"16": "icon.png",
			"48": "icon.png",
			"128": "icon.png"
		}
	},
	"background": {
		"service_worker": "src/background.js"
	}
}
