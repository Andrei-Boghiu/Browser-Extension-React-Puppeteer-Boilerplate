{
	"extends": "./node_modules/gts/tsconfig-google.json",
	"compilerOptions": {
		"lib": ["DOM"],
		"rootDir": ".",
		"outDir": "build",
		"jsx": "react",
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"target": "ES6"
	},
	"include": ["lib/**/*.ts", "src/**/*.ts", "src/**/*.tsx", "src/**/*.json", "src/popup/popup.jsx"]
}
