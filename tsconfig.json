{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "lib": ["DOM"],
    "rootDir": ".",
    "outDir": "build",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "target": "ES6"
  },
  "include": [
    "lib/**/*.ts",
    "src/**/*.ts", 
    "src/**/*.tsx",
    "src/**/*.json",
    "public/**/*.png"
  ]
}
