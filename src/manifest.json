{
	"name": "Puppeteer Extension",
	"manifest_version": 3,
	"version": "0.0.0.1",
	"description": "Example extension puppeteer-extension-transport",
	"permissions": ["tabs", "debugger", "activeTab"],
	"host_permissions": ["<all_urls>"],
	"action": {
		"default_popup": "popup.html"
	},
	"background": {
		"service_worker": "background.js"
	},
	"commands": {
		"start": {
			"suggested_key": {
				"default": "Alt+A"
			},
			"description": "for testing puppeteer-extension-transport"
		}
	}
}
